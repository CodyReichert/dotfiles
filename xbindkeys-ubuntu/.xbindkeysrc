#volUp
"amixer -q set Master playback 1%+"
    m:0x10 + c:123
    Mod2 + XF86AudioRaiseVolume 

#volDown
"amixer -q set Master playback 1%-"
    m:0x10 + c:122
    Mod2 + XF86AudioLowerVolume 

#cmus play/pause
"cmus-remote -u"
    m:0x10 + c:127
    Mod2 + Pause 

#cmus stop
"cmus-remote -s"
    m:0x11 + c:127
    Shift+Mod2 + Pause 

#cmus next
"cmus-remote -n"
    m:0x10 + c:75
    Mod2 + F9 

#cmus previous
"cmus-remote -r"
    m:0x0 + c:74
    F8 

