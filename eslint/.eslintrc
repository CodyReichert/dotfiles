{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "es6": true,
        "jquery": true,
        "amd": true,
        "node": true
    },
    "ecmaFeatures": {
        "arrowFunctions": true,
        "blockBindings": true,
        "classes": true,
        "defaultParams": true,
        "destructuring": true,
        "forOf": true,
        "generators": true,
        "modules": true,
        "spread": true,
        "templateStrings": true,
        "jsx": true
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "consistent-return": [0],
        "key-spacing": [0],
        "quotes": [0],
        "new-cap": [0],
        "no-multi-spaces": [0],
        "no-shadow": [0],
        "no-unused-vars": [1],
        "no-use-before-define": [2, "nofunc"],
        "react/jsx-uses-vars": 1
    }
}
